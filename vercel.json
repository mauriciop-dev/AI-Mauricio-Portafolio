{
  "build": {
    "env": {}
  },
  "crons": [],
  "routes": [
    
    {
      "src": "/videos/(.*)\\.mp4",
      "headers": {
    
        "Content-Type": "video/mp4",
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/public/videos/$1.mp4"
    }
    
  ],
  "git": {
    "deploymentEnabled": true,
    "projectId": "auto" 
  },
  "framework": "nextjs", 
  "outputDirectory": ".next" 
}